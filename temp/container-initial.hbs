{{!-- {{#each this}} --}}
<div class="container-{{this}} container container-st-p">
	<div class="header-onsale-cont">

		<button type="button" class="left-button" data-left>
			<span class="material-icons">
				chevron_left
			</span>
		</button>

		<span>
			<h1 class="cont-name-{{this}} category-title">{{category}}</h1>
		</span>

		<a class="link" href=""><span class="current-page" data-category="{{this}}">Дивитися
				всі</span>
		</a>

		<button type="button" class="right-button-st-p" data-right>
			<span class="material-icons">
				chevron_right
			</span>
		</button>

	</div>

	<div>
		<ul class='cont-initial-{{this}} catigories-container js-catigories'></ul>
	</div>
</div>
{{!-- {{/each}} --}}